  },
  "homepage": "https://github.com/valentinuuiuiu/Human-AI-Collaboration#readme",
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.0",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@activepieces/pieces-framework": "^0.20.0",
