{
  "name": "human-ai-collaboration",
  "version": "1.0.0",
  "description": "Advanced LLM orchestration tools and workflows for seamless Human-AI collaboration",
  "main": "index.js",
  "scripts": {
    "build": "npm run build:llm-orchestrator && npm run build:mcp-server",
    "build:llm-orchestrator": "cd llm-orchestrator && npm run build",
    "build:mcp-server": "cd mcp-server && npm run build",
    "install:all": "npm install && cd llm-orchestrator && npm install && cd ../mcp-server && npm install",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "echo \"Linting coming soon!\" && exit 0"
  },
  "keywords": [
    "llm",
    "ai",
    "automation",
    "workflow",
    "activepieces",
    "flowise",
    "n8n",
    "mcp",
    "openai",
    "anthropic",
    "collaboration"
  ],
  "author": "Human-AI Collaboration Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/valentinuuiuiu/Human-AI-Collaboration.git"
  },
  "bugs": {
